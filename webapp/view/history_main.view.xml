<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml" controllerName="hackaton.controller.history_main">
	<App>
		<pages>
			<Page title="Medical history" showNavButton="true" navButtonPress="onNavBack">
				<content>
				    <List noDataText="Drop list items here" id="historyList" items="{/userHistory}">
				        <items>
				            <StandardListItem type="Navigation" id="__item0" title="{HISTORYTYPE}" description="{COMMENT}"/></items>
				    </List>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>